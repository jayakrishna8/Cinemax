<div class="booyah-box col-10 offset-1">
  <h1>Add A New Lesson</h1>
  <%= simple_form_for @lesson, url: instructor_courses_path do |f| %>
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :video %>
    <br />
    <%= f.submit "Add Lesson", class: 'btn btn-primary' %>
    <br />
    <br />
  <% end %>
</div>
